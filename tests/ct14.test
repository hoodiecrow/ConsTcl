package require tcltest
source constcl.tcl
namespace import -force tcltest::*


test eval-7.0 {try eval-ing a sequence} -body {
  pew "(begin (display 'a) (display 'b))"
} -output "ab"

test eval-7.1 {try eval-ing a sequence} -body {
  set x [p "(begin (define a 3) (define b 4) (display a) (display b))"]
  set v [::constcl::eval $x [::constcl::MkEnv ::constcl::global_env]]
} -output "34"


cleanupTests
return
