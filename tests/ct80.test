package require tcltest
source constcl.tcl
namespace import -force tcltest::*


test control-3.0 {try map)} -body {
    pew {(map cadr '((a b) (d e) (g h)))}
    pew {(map (lambda (n) (expt n n)) '(1 2 3 4 5))}
    pew {(map + '(1 2 3) '(4 5 6))}
    pew {(let ((count 0))
  (map (lambda (ignored)
         (set! count (+ count 1))
         count)
       '(a b)))}
} -output "(b e h)\n(1.0 4.0 27.0 256.0 3125.0)\n(5 7 9)\n(1 2)\n"



cleanupTests
return
