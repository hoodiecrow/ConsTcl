
package require tcltest
namespace import -force tcltest::*
source ../constcl.tcl

test control-4.0 {try for-each)} -body {
    pew {(for-each display '(1 2 3))}
} -output "123()\n"

test control-4.1 {try for-each)} -body {
    pew {(let ((v (make-vector 5)))
  (for-each (lambda (i)
              (vector-set! v i (* i i)))
            '(0 1 2 3 4))
  v)}
} -output "#(0 1 4 9 16)\n"


cleanupTests
return
