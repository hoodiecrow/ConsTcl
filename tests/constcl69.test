
package require tcltest
namespace import -force tcltest::*
source ../constcl.tcl

test eval-9.0 {try eval-ing a define} -body {
  pew "(define r 10)"
  pew "r"
} -output "10\n"

test eval-9.1 {try eval-ing a define, with an inappropriate symbol} -body {
  pew "(define let 10)"
} -returnCodes error -result "Variable name is reserved: let"

test eval-9.2 {try eval-ing a define, with an inappropriate symbol} -body {
  pew "(define +foo 10)"
} -returnCodes error -result "Identifier expected (+foo)"

cleanupTests
return
