
package require tcltest
namespace import -force tcltest::*
source constcl.tcl

test write-1.0 {read, eval, and write a number} -body {
    pew "99.99"
} -output "99.99\n"

test write-1.1 {read, eval, and write a boolean} -body {
    pew "#t"
} -output "#t\n"

test write-1.2 {read, eval, and write a list} -body {
    pew "'(a b c)"
} -output "(a b c)\n"

test write-1.3 {read, eval, and write a list} -body {
    pew "'(a b (c))"
} -output "(a b (c))\n"


cleanupTests
return
