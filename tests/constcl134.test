
package require tcltest
namespace import -force tcltest::*
source ../constcl.tcl

test pairslists-1.0 {playing with lists} -body {
    pew {(define x (list 'a 'b 'c))}
    pew {(define y x)}
    pew {y}
    pew {(list? y)}
} -output "(a b c)\n#t\n"

test pairslists-1.2 {playing with lists} -body {
    pew {(set-cdr! x 4)}
    pew {x}
} -output "(a . 4)\n(a . 4)\n"

test pairslists-1.3 {playing with lists} -body {
    pew {(eqv? x y)}
    pew {y}
} -output "#t\n(a . 4)\n"

test pairslists-1.4 {playing with lists} -body {
    pew {(eqv? x y)}
    pew {y}
    pew {(list? y)}
} -output "#t\n(a . 4)\n#f\n"



cleanupTests
return
