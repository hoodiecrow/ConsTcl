package require tcltest
source constcl.tcl
namespace import -force tcltest::*


test numbers-18.0 {try abs} -body {
    pew "(abs -99)"
} -output "99\n"

test numbers-18.1 {try check} -body {
    pew "(abs \"foo\")"
} -returnCodes error -result "NUMBER expected\n(abs \"foo\")"


cleanupTests
return
