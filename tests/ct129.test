package require tcltest
source constcl.tcl
namespace import -force tcltest::*


test cons-15.0 {check for a symbol} -body {
    pew "pi"
} -output 3.1415926535897931\n

test cons-15.1 {dereference an unbound symbol} -body {
    pew "foo"
} -returnCodes error -result "not a variable name (foo)"

test cons-15.2 {dereference an unbound symbol: procedure} -body {
    pew "(foo)"
} -returnCodes error -result "unbound symbol (foo)"


cleanupTests
return
