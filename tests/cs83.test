
package require tcltest
namespace import -force tcltest::*
source constcl.tcl

test control-2.0 {try apply)} -body {
    pew {(apply + (list 3 4))}
    pew {(define compose
  (lambda (f g)
    (lambda args
      (f (apply g args)))))}
    pew {((compose sqrt *) 12 75)}
} -output "7\n30.0\n"

test control-2.1 {try triggering a check} -body {
    pew {(apply #\+ (list 3 4))}
} -returnCodes error -result "PROCEDURE expected\n(apply #\\+ ...)"



cleanupTests
return
