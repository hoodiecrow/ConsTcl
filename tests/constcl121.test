
package require tcltest
namespace import -force tcltest::*
source ../constcl.tcl

test boolean-1.0 {evaluate boolean values} -body {
        pew "#t"
} -output "#t\n"

test boolean-1.1 {evaluate boolean values} -body {
        pew "#f"
} -output "#f\n"

test boolean-1.2 {evaluate boolean values} -body {
        pew "'#f"
} -output "#f\n"

test boolean-1.3 {equivalence of instance and constant} -body {
        expr {[::constcl::MkBoolean "#t"] == ${#t}}
} -result 1

test boolean-1.5 {equivalence of instance and constant} -body {
        expr {[pe "(> 3 2)"] == ${#t}}
} -result 1

test boolean-1.6 {equivalence of instance and constant} -body {
        expr {[pe "#t"] == ${#t}}
} -result 1



cleanupTests
return
