    pew "(number->string 23)"
    pew "(number->string 23 2)"
    pew "(number->string 23 8)"
    pew "(number->string 23 16)"
} -output "\"23\"\n\"10111\"\n\"27\"\n\"17\"\n"

test numbers-30.1 {try number->string} -body {
