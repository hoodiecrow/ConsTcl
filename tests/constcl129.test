
package require tcltest
namespace import -force tcltest::*
source ../constcl.tcl

test io-1.0 {try call-with-input-file} -setup {
  makeFile {99 foo "bar"} iofile.txt
} -body {
  pe {(let ((f (lambda (port)  (let ((n 0) (s 0))
       (set! n (read port))
       (set! s (read port))
       (display s)
       (display n)))))
