package require tcltest
source constcl.tcl
namespace import -force tcltest::*


test eval-1.0 {try eval-ing a symbol} -body {
  pew "(let ((r 10)) r)"
} -output "10\n"

test eval-1.2 {try eval-ing an unbound symbol} -body {
  pew "k"
} -returnCodes error -result "not a variable name (k)"


cleanupTests
return
