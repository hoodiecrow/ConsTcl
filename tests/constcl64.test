
package require tcltest
namespace import -force tcltest::*
source ../constcl.tcl

test eval-4.0 {try eval-ing a conditional} -body {
  pew "(if (= 2 2) (display 'a) (display 'b))"
} -output "a"

test eval-4.1 {try eval-ing a short conditional} -body {
  pew "(if (= 2 3) (display 'a))"
} -output ""

cleanupTests
return
