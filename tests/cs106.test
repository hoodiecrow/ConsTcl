
package require tcltest
namespace import -force tcltest::*
source constcl.tcl

test pairslists-16.0 {try memq, memv} -body {
    pew {(memq 'a '(a b c))}
    pew {(memq 'b '(a b c))}
    pew {(memq 'a '(b c d))}
    pew {(memq (list 'a) '(b (a) c))}
    pew {(memq 101 '(100 101 102))}
    pew {(memv 101 '(100 101 102))}
} -output "(a b c)\n(b c)\n#f\n#f\n(101 102)\n(101 102)\n"

test pairslists-16.1 {try member} -body {
    pew {(member (list 'a) '(b (a) c))}
} -output "((a) c)\n"


cleanupTests
return
